    "ava": "ava -vs test/*.test.js",
    "test": "npm run ava"
  },
  "dependencies": {
    "debug": "2.6.4"
  },
  "devDependencies": {
    "ava": "0.19.1",
    "express": "4.15.3",
    "babel-cli": "6.22.2",
