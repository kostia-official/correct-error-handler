    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.22.0",
    "body-parser": "1.17.1",
    "nyc": "10.2.0",
    "require-subvert": "0.1.0",
    "sinon": "2.1.0",
    "supertest": "3.0.0"
  },
  "optionalDependencies": {
    "rollbar": "2.0.2"
  },
  "ava": {
    "require": "babel-register",
    "babel": {
      "presets": [
        "es2015",
        "stage-0"
      ],
      "plugins": [
        "transform-runtime"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kozzztya/correct-error-handler.git"
  },
  "keywords": [
    "express",
    "error",
    "handler"
  ],
  "author": "kozzztya",
